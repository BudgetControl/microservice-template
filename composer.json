{
    "name": "budgetcontrol/stats",
    "description": "defines the dependencies required for a PHP microservice in a Budget control application",
    "type": "none",
    "license": "MIT",
    "repositories": {
        "source": {
            "type": "vcs",
            "url": "https://github.com/mlabfactory/phpunit-self-api-integration.git"
        }
    },
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.13",
        "doctrine/dbal": "^4.0",
        "symfony/var-dumper": "^7.0",
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^11.0",
        "laravel/serializable-closure": "^1.3",
        "symfony/serializer": "^7.0",
        "robmorgan/phinx": "^0.16.0",
        "slim/http": "^1.3",
        "slim/psr7": "^1.6",
        "psr/log": "^3.0",
        "illuminate/events": "^11.0",
        "symfony/property-access": "^7.0",
        "illuminate/filesystem": "^11.0",
        "nesbot/carbon": "^3.2",
        "logtail/monolog-logtail": "^3.1"
    },
    "require-dev": {
        "nikic/php-parser": "^5.0",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^10.0",
        "mlabfactory/phpunit-self-api-integration": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Budgetcontrol\\MSNAME\\": "src/"
        },
        "files": [
            "config/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Marco De Felice",
            "email": "marco.defelice@mlabfactory.it"
        }
    ],
    "minimum-stability": "stable"
}
